{
    "manifest_version": 3,
    "name": "Dynamic Home Page",
    "version": "1.0",
    "description": "A custom homepage with a dynamic background.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [],
    "web_accessible_resources": [
        {
            "resources": ["images/*"],
            "matches": ["<all_urls>"]
        }
    ]
}
