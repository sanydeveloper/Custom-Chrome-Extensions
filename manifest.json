{
    "manifest_version": 3,
    "name": "Dynamic Home Page",
    "version": "1.0",
    "description": "Changes home page background and plays music on every start.",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [],
    "web_accessible_resources": [
      {
        "resources": ["images/*", "music/*"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  